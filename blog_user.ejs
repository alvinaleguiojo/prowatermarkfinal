                border-bottom: 8px solid #3f51b5;
                border-radius: 5px;
            }

            body{
                justify-content:center;
                align-items: center;     
            }
            
            .main-div {
                display: flex;
                justify-content:center;
                align-items: center;
            }
            @media screen and(min-width:1000px) {
                .container_body{
                    width: 100%;
                    height: 100vh;
                }
            }
            .container_body {
                margin-top: 100px;
            }

            .sidebar {
                
                padding: 10px 10px;
                min-width: 50px;
            }
            .widget {
              
                padding: 10px 10px;
                min-width: 50px;
            }
            .inside{
                border: none;
                border-radius: 10px;
                z-index: 1;
                word-wrap: break-word;
                padding: 15px 15px;
                width: 100%;
                justify-content: center;
                align-items: center;
                box-shadow: rgba(0, 0, 0, 0.16) 0px 1px 4px;
                max-width: 600px;
                margin-bottom: 10px;
            }

            img{
              max-width: 100%;
              max-height: 660px;
            }
         
           
            .btn-outline-primary{
            border-color: #3f51b5;
            color: #3f51b5;
             }
        .btnn{
            background-color: #3f51b5;
            color: white;
        }
        .btn-primary{
            background-color: #3f51b5;
            color: white;
        }
        .btn-block:hover{
            background-color: #283686;
            color: white;
        }
        .inside > h2{
                letter-spacing: 4px;
                font: 20px "Lato", sans-serif;
                color: #111;
                margin-top: 0.9em;
                
            }
            .inside > p{
                font: 400 15px/1.8 "Lato", sans-serif;
                margin-top: 1em;
                margin-bottom: 1em;
                text-align: left;
                color:black;
            }
            @media only screen and (max-width: 768px) {
        /* For mobile phones: */
        [class*="col-"] {
          text-align: center;
          margin-bottom: 1em;
        }
        .inside{
                width: 100%;
                max-width: 339px;
               }
      }
    </style>
</head>

